<table border = "1">
<tr>



<%= form_tag search_events_path, :method => :get do %>
<td>	<%= text_field_tag "keyword" %>
	<%= submit_tag "Search" %></td>
<% end %> 

<td>
<%= link_to 'Sort by Name', events_path( :order => "name", :xxx => "Yyy") %>
</td>
<td>
<%= link_to 'Sort by Default', events_path %>
</td>
</tr>
<tr>
<td><%= paginate @events %></td>
</tr>
<%#= debug(@event) %>


<!-- <ul> -->

<% @events.each do |event| %>
  <tr>
     <%#= event.name %>
  <%#= link_to 'Show', :controller => 'events', :action => 'show', :id => event %>
  <%#= link_to 'Edit', :controller => 'events', :action => 'edit', :id => event %>
  <%#= link_to 'Delete', :controller => 'events', :action => 'destroy', :id => event %>




    <td><%= link_to event.name, event_path(event) %></td>
    <td><%= link_to " (XML)", event_path(event, :format => :xml) %></td>
    <td><%= link_to " (JSON)", event_path(event, :format => :json) %></td>
    <td><%= link_to 'edit', edit_event_path(event) %></td>
    <td><%= button_to 'delete', event_path(event), :method => :delete %></td>

   

    <td><%= link_to 'Dashboard', dashboard_event_path(event) %></td>
  </tr>
<% end %>

<!-- </ul> -->
<tr>
<td><%= paginate @events %></td>
</tr>
<%#= link_to 'new event', new_event_path %>
<tr>
<td><%= link_to "Atom feed", events_path(:format => :atom) %></td>
</tr>
<%#= link_to 'admin', admin_event_path(event) %>
<tr>
<%= form_for @event, :url => events_path do |f| %>
    <td><%= render :partial => 'form', :locals => { :f => f } %></td>
    <td><%= f.submit "Create" %></td>
<% end %>
</tr>
</table>
